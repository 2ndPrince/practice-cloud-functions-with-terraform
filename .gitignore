# Ignore node_modules
node_modules
.idea

################### .gitignore for Terraform ###################

# Local .terraform directories
**/.terraform/*
**/.terraform/

# .tfstate files
*.tfstate
*.tfstate.*

# Crash log files
crash.log

# Exclude Terraform lock file
.terraform.lock.hcl

# Exclude backup files
*.backup

# Exclude override files
override.tf
override.tf.json
*_override.tf
*_override.tf.json

# Exclude CLI configuration files
.terraformrc
terraform.rc

############################################################################

src/*.js